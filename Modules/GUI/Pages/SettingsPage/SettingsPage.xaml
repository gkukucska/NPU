<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:NPU.Pages.SettingsPage"
             x:Class="NPU.Pages.SettingsPage.SettingsPage"
             x:DataType="local:SettingsPageViewModel"
             Title="SettingsPage">
    <VerticalStackLayout>
        <Button Text="Logout"
                HeightRequest="50"
                WidthRequest="100"
                Command="{Binding LogoutCommand}"
                Margin="10"
                CornerRadius="5"/>
    </VerticalStackLayout>
</ContentPage>